{
  "name": "notitia",
  "version": "1.1.4",
  "description": "Convert Regular HTML Article Pages into Google AMP Pages, Facebook Instant Articles or Apple Article Format",
  "main": "index.js",
  "scripts": {
    "start": "node bin/dev",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && babel src -s -d dist",
    "production": "npm run build && node bin/production",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmadde10/Notitia.git"
  },
  "keywords": [
