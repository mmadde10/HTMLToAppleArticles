    "amp",
    "facebook",
    "instant",
    "article",
    "converter",
    "ampit",
    "htmltoAMP",
    "htmltoFaceBookInstantArticle",
    "Accerated Mobile Pages",
    "Apple Article",
    "Apple"
  ],
  "author": "Mark Madden",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mmadde10/Notitia/issues"
  },
  "homepage": "https://github.com/mmadde10/Notitia#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "jest": "^21.2.1",
    "regenerator-runtime": "^0.11.0",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
